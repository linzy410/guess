			http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
			http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

	<bean id="wordsDao" class="com.hisupplier.guess.dao.WordsDao" autowire="byName" />
	<bean id="sessionDao" class="com.hisupplier.guess.dao.SessionDao" autowire="byName" />
	<bean id="userDao" class="com.hisupplier.guess.dao.UserDao" autowire="byName" />
	<bean id="userService" class="com.hisupplier.guess.service.UserService" autowire="byName" />

